{
    "name": "claude-proxy-status",
    "displayName": "Antigravity Proxy Status",
    "description": "Multi-account management and model switching for Antigravity Claude Proxy - view account status, switch models, manage sessions",
    "version": "3.3.0",
    "publisher": "ai-dev-2024",
    "repository": {
        "type": "git",
        "url": "https://github.com/ai-dev-2024/Antigravity-Claude-Code-Proxy"
    },
    "icon": "icon.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "antigravity",
        "claude",
        "proxy",
        "google ai",
        "gemini",
        "multi-account",
        "quota"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "antigravity.openDashboard",
                "title": "Antigravity: Open Dashboard"
            },
            {
                "command": "antigravity.switchModel",
                "title": "Antigravity: Switch Model"
            },
            {
                "command": "antigravity.showCurrentModel",
                "title": "Antigravity: Show Current Model"
            },
            {
                "command": "antigravity.switchAccount",
                "title": "Antigravity: Switch Account"
            },
            {
                "command": "antigravity.addAccount",
                "title": "Antigravity: Add Account"
            },
            {
                "command": "antigravity.refreshAccounts",
                "title": "Antigravity: Refresh Account Status"
            }
        ]
    }
}